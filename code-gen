<script type = "text/javascript">
function generateLIHP() {
var path = document.forms.input.imagePath.value;
var link = document.forms.input.link.value;
var alt = document.forms.input.alt.value;
var output = document.forms.input.output;
output.value = "<style type='text/css'>.bg-clicker {height: 0px;width: 990px;position: relative;z-index: -1;top: -120px;}.tablet .bg-clicker{height: 0px;width: 990px;position: absolute;z-index: -1;top: 210px;}.tablet .home #promoContainer {   padding-top: 0;}.guest .bg-clicker {display: none;}</style><script type='text/javascript'>var appDivBefore = document.getElementById('contentPane');var appDivInside = document.getElementById('container');var bgdiv = document.createElement('div');bgdiv.setAttribute('class', 'bg-clicker');appDivInside.insertBefore(bgdiv, appDivBefore);var aTag = document.createElement('a');aTag.setAttribute('href','"+link+"');bgdiv.appendChild(aTag);var bgImg = document.createElement('img');bgImg.setAttribute('src','"+path+"');bgImg.setAttribute('height','120px');bgImg.setAttribute('width','990px');bgImg.setAttribute('alt','"+alt+"');aTag.appendChild(bgImg);</scr"+"ipt>";
}
function showPage(sel, button) {
if(button.className.indexOf('active') > -1) {
return;
}
var buttons = document.querySelectorAll('.menu-btn');
for (p = 0; p < buttons.length; p++) {
buttons[p].className = 'menu-btn';
}
var pages = document.querySelectorAll('.container');
for(i = 0; i < pages.length; i++) {
if(pages[i].id === sel) {
pages[i].style.display = 'block';
} else {
pages[i].style.display = 'none';
}
}
button.className = 'menu-btn active';
}
function checker(checkbox) {
if(checkbox.checked && checkbox.parentNode.className.indexOf('checked') === -1) {
checkbox.parentNode.className = 'ssCheck checked';
} else if (!checkbox.checked && checkbox.parentNode.className.indexOf('checked') > -1) {
checkbox.parentNode.className = 'ssCheck';
} else {
return;
}
}
function resetCheck() {
var chk = document.querySelectorAll('.ssInputCheck');
for (i = 0; i < chk.length; i++) {
chk[i].checked = false;
}
}
function nextWed() {
	var date = new Date();
	date.setDate(date.getDate() + (3 - 1 - date.getDay() + 7) % 7 + 1 + 13);
	var day = date.getDate();
	var monthNames = ["January", "February", "March", "April", "May", "June","July", "August", "September", "October", "November", "December"];
	var month = monthNames[date.getMonth()];
	var year = date.getFullYear();
	document.getElementById('ssDate').value = day + ' ' + month + ' ' + year;
	console.log(day + ' ' + month + ' ' + year);
}
function setAttributes(el, attrs) {
  for(var key in attrs) {
    el.setAttribute(key, attrs[key]);
  }
}
function generateSS() {
var ssInsert = document.getElementById('ssOutputWrap');
ssInsert.innerHTML = ' ';
var allCheckBoxes = [];
var checkBoxes = document.querySelectorAll('.ssInputCheck');
var ssLabel = document.createElement('div');
ssLabel.setAttribute('class', 'label-col');
var ssFDWrap = document.createElement('div');
ssFDWrap.setAttribute('class', 'threeCol');
var ss10pcWrap = document.createElement('div');
ss10pcWrap.setAttribute('class', 'threeCol');
var ss10DollWrap = document.createElement('div');
ss10DollWrap.setAttribute('class', 'threeCol');
var SSdate = document.getElementById('ssDate').value;
for (i=0;i<checkBoxes.length;i++) {
  if (checkBoxes[i].checked) {
    allCheckBoxes.push(checkBoxes[i]);
  }
}
for (j=0;j<allCheckBoxes.length;j++) {
var ssLabelDiv = document.createElement('p');
ssLabelDiv.setAttribute('class', 'inline segmentLabel');
ssLabelDiv.innerHTML = '$' + allCheckBoxes[j].value;
ssLabel.appendChild(ssLabelDiv);
}
var heading = document.createElement('p');
heading.innerHTML = 'Free Delivery';
ssFDWrap.appendChild(heading);
for (j=0;j<allCheckBoxes.length;j++) {
var ssFDoutDiv = document.createElement('input');
setAttributes(ssFDoutDiv, {'class': 'ssOutput', 'type': 'text', 'readonly': 'readonly', 'onclick': 'this.select();'});
